#include <stdio.h>

int anio;
int bisiesto(int anio);

int main()
{
	printf("\nIngrese un anio para saber si es bisiesto o no: \n");
	scanf("%d",&anio);
	bisiesto(anio);
}

int bisiesto(int anio)
{
	if(anio % 4 == 0)
	{
		printf("El anio es bisiesto.\n");
	}else{
		printf("El anio no es bisiesto.\n");
	}
	return 0;
}
