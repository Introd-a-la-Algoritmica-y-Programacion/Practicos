#include <stdio.h>

int n, i, cuenta;

int main(){
	printf("Introduce el numero a averiguar si es primo o no:");
	scanf("%d",&n);

	for(i = 1; i <=n; i++){
		if(n % i == 0){
			cuenta++;
		}
	}
	if(cuenta == 2){
		printf("El numero es primo");
	}else{
		printf("El numero no es primo");
	}
	return 0;
}
